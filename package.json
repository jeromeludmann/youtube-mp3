{
  "name": "youtubemp3",
  "version": "0.6.0",
  "description": "Get and slice MP3 from Youtube",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src/ --out-dir dist/ --minified",
    "clean": "rimraf dist/",
    "dev": "node -r babel-register src/cli/index.js",
    "prepublish": "npm run clean && npm run build"
  },
