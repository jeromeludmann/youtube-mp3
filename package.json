  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-runtime": "^6.26.0",
    "chai": "^3.5.0",
    "mocha": "^3.3.0"
  },
  "dependencies": {
    "rimraf": "^2.6.1",
    "shelljs": "^0.8.1",
    "yargs": "^11.0.0"
  }
}
