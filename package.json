{
  "name": "youtube-to-mp3",
  "version": "0.4.3",
  "description": "Get and slice MP3 from Youtube",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "npm run build",
    "update-youtube-dl": "sh ./update-youtube-dl.sh",
    "build": "babel src/ --out-dir dist/ --minified",
    "clean": "rimraf dist/",
    "dev": "node -r babel-register src/cli.js"
  },
  "bin": {
    "youtube-to-mp3": "./dist/cli.js"
  },
  "keywords": [
    "youtube",
    "mp3",
    "slice",
    "download"
  ],
  "author": "Jerome Ludmann",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-runtime": "^6.23.0",
    "chai": "^3.5.0",
    "mocha": "^3.3.0"
  },
  "dependencies": {
    "optimist": "^0.6.1",
    "rimraf": "^2.6.1",
    "yargs": "^11.0.0"
  }
}
