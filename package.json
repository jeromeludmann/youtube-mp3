{
  "name": "youtube-mp3-slicer",
  "version": "0.2.3",
  "description": "Get and slice MP3 from Youtube",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "npm run build; mkdir -p bin; curl -L https://yt-dl.org/downloads/latest/youtube-dl -o bin/youtube-dl; chmod +x bin/youtube-dl",
    "build": "babel src/ --out-dir dist/ --minified",
    "clean": "rimraf dist/ bin/",
    "dev": "node -r babel-register src/test.js"
  },
  "keywords": [
    "youtube",
    "mp3",
    "slice",
    "download"
  ],
  "author": "Jerome Ludmann",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-runtime": "^6.23.0",
    "chai": "^3.5.0",
    "mocha": "^3.3.0",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "express": "^4.15.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "socket.io": "^1.7.3"
  }
}
